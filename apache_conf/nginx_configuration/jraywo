server {
	listen		80; 
	server_name jraywo.no-ip.ca;
	#server_name 127.0.0.1:80;

	# Allow file uploads
	client_max_body_size 50M;

	location /static {
		autoindex on;
		alias 	/var/www/django_projects/static;
	}

	location /media {
		autoindex on;
		alias 	/var/www/django_projects/media;
	}

	location / {
 		proxy_pass_header Server;
                proxy_set_header Host $http_host;
                proxy_redirect off;
                proxy_set_header X-Real-IP $remote_addr;
                proxy_set_header X-Scheme $scheme;
                proxy_pass http://jraywoes;
	}

}

